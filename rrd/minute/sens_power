<RRD::GRAPH %%WEBDIR%%/pix_%%SENSDEV%%/%%MONTERM%%m.png
        --imginfo '<IMG SRC=pix_%%SENSDEV%%/%s WIDTH=%lu HEIGHT=%lu >'
	-a PNG
	-h 200 -w 500
	--lazy
	-s -10minutes
	-v "Power (watt)"
	-t %%CHARTDESC%%
	-x MINUTE:1:MINUTE:3:MINUTE:3:0:'%b %d %H:%M'
	--lower-limit 0
	-Y
	DEF:temp=%%RRDDIR%%/sensors_%%SENSDEV%%.rrd:%%MONTERM%%:AVERAGE
	LINE2:temp#FF00FF>

